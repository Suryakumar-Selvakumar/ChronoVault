<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Mini Message Board</title>
  </head>
  <body>
    <a href="/new">Post a message!</a>
    <div class="messages">
      <% messages.forEach((message, index) => {%> <%-
      include("messages/message", {message: message, index: index+1}) %> <% });
      %>
    </div>
  </body>
</html>
